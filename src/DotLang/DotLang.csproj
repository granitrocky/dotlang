<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <PackageId>Graphviz.DotLanguage</PackageId>
    <Description>Graphviz.DotLanguage provides a full fidelity lexer, recursive descent parser, and abstract syntax tree for the Graphviz DOT Language. Use it to parse graphs described in the DOT Language or build graphs using the API and serialize to the DOT Language.</Description>
  </PropertyGroup>
</Project>
